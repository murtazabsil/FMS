<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>FMS2</display-name>
  <welcome-file-list>
    <welcome-file>jsp/Home.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>com.bluestar.fms.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Admin</servlet-name>
    <servlet-class>com.bluestar.fms.servlet.AdminServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Admin</servlet-name>
    <url-pattern>/Admin</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ActionServlet</servlet-name>
    <servlet-class>com.bluestar.fms.servlet.ActionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ActionServlet</servlet-name>
    <url-pattern>/ActionServlet/*</url-pattern>
  </servlet-mapping>
  <filter>
    <display-name>BrowserType</display-name>
    <filter-name>BrowserType</filter-name>
    <filter-class>com.bluestar.filters.BrowserType</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>BrowserType</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>ForecastServlet</display-name>
    <servlet-name>ForecastServlet</servlet-name>
    <servlet-class>com.bluestar.fms.servlet.ForecastServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ForecastServlet</servlet-name>
    <url-pattern>/ForecastServlet</url-pattern>
  </servlet-mapping>
</web-app>